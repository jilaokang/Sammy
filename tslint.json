{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "semicolon": true,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "no-empty": false,
    "no-console": false
  }
}
